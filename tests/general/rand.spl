Int seed = 2340923242;

Int rand() {
	Int x = (seed * 1103515245) + 12345;
	if (x > exp(2, 31))
		x = x - exp(2, 31);
	return x; 
}

Int exp(Int x, Int n) {
	if (n == 0) return 1;
	else if (n == 1) return x;
	else if (even(n)) return exp(x * x, n / 2);
	else if (odd(n)) return x * exp(x * x, (n - 1) / 2);
	else return exp(1 / x, -n);
}

Bool even(Int x) {
	return x % 2 == 0;
}

Bool odd(Int x) {
	return x % 2 != 0;
}
